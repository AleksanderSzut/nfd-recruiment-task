{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": true,
        "braces": false,
        "new_with_braces": {
            "anonymous_class": false,
            "named_class": false
        },
        "return_assignment": true,
        "declare_strict_types": true,
        "strict_comparison": true,
        "no_useless_else": true,
        "yoda_style": true
    }
}
